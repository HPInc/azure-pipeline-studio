{
  "configurations": [
    {
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "env": {
        "NODE_ENV": "debug"
      },
      "name": "Run Extension",
      "outFiles": [
        "${workspaceFolder}/extension.js"
      ],
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "type": "extensionHost"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node App",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/extension.js",
      "args": [
        "/root/workspace/NCE/perform-sdk/codeway.yaml",
        "-r", "templates=/root/workspace/templates/",
        "-v", "Build.Reason=Manual",  "-v", "Build.SourceBranch=refs/heads/p1", "-v", "Build.SourceBranchName=p1", "-v", "System.TeamFoundationCollectionUri=https://dev.azure.com/hp", "-v", "System.TeamProject=OneClient",
        "-x", "-f", "-a",
        "-o", "/tmp/output.yaml"
      ]
    }
  ],
  "version": "0.2.0"
}
